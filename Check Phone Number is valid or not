// You are using GCC
#include<iostream>
#include<string>

using namespace std;
int main(){
    string str;
    cin >> str;
    int length = str.size();
    if(length == 12){
        cout << "Yes, the phone number is valid." << endl;
        for(int i=0;i<length;i++){
            if(str[i] == '-'){
                continue;
            }
            cout << str[i] ;
        }
    }
    else{
        cout << "No, the phone number is not valid." << endl;
    }
    
    return 0;
}
